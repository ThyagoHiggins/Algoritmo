Algoritmo "somador"

Var
   contador, soma, n: inteiro

Inicio
     contador <- 1
     soma <- 0
     enquanto (contador <=5) faca
     escreva ("Digite o", contador,"o. valor:")
     leia (n)
     
     contador <- contador + 1
     soma <- soma+n
     
     fimenquanto
     
     escreval ("A soma é: ", soma)


Fimalgoritmo